package newPackage;

import org.openqa.selenium.By;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.ui.Select;

public class WebElementClass extends TestClass {
	
	public void dropBox(String path) {
	WebElement drpSource = driver.findElement(By.xpath(path));
	Select Source = new Select(drpSource);
	Source.selectByVisibleText("San Diego");
	}
}
